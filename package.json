{
  "name": "@supergrid9k/web",
  "version": "0.0.1",
  "homepage": "https://supergrid9k.dev",
  "description": "A visual way to generate css for all your css-grid layouts.",
  "keywords": [
    "css",
    "grid",
    "css-grid",
    "generator",
    "layout",
    "create"
  ],
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/",
      "/.next/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "collectCoverageFrom": [
      "./*.{js,ts}"
    ]
  },
  "dependencies": {
    "@primer/components": "^20.3.0",
    "@primer/octicons-react": "^11.0.0",
    "@rebass/forms": "^4.0.6",
    "@rebass/layout": "^4.0.6",
    "axios": "^0.20.0",
    "babel-jest": "^26.5.2",
    "clipboard": "^2.0.6",
    "colors": "^1.4.0",
    "envalid": "^6.0.2",
    "firebase": "^7.24.0",
    "firebase-admin": "^9.2.0",
    "formik": "^2.2.0",
    "framer-motion": "^2.9.1",
    "jest": "^26.5.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.9",
    "next": "^9.5.5",
    "next-connect": "^0.9.0",
    "next-cookies": "^2.0.3",
    "polished": "^4.0.3",
    "prism-react-renderer": "^1.1.1",
    "react": "^17.0.0",
    "react-clipboard.js": "^2.0.16",
    "react-div-100vh": "^0.5.6",
    "react-dom": "^17.0.0",
    "react-dropdown-select": "^4.6.0",
    "react-error-boundary": "^3.0.2",
    "react-firebaseui": "^4.1.0",
    "react-json-view": "^1.19.1",
    "react-typography": "^0.16.19",
    "react-use": "^15.3.4",
    "rebass": "^4.0.7",
    "recoil": "^0.0.13",
    "styled-components": "^5.2.0",
    "styled-system": "^5.1.5",
    "swr": "^0.3.6",
    "ts-jest": "^26.4.1",
    "typescript": "^4.1.0-dev.20201021",
    "typography": "^0.16.19",
    "typography-plugin-code": "^0.16.19",
    "use-debounce": "^5.0.1"
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "build": "next build",
    "export": "next export",
    "format": "prettier --ignore-path .gitignore --write \"./**/*.{ts,tsx,json,yml,md}\"",
    "lint:js": "eslint --ignore-path .gitignore \"./**/*.{ts,tsx}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"./**/*.{ts,tsx}\"",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run format && npm run lint:js --fix && npm run lint:style",
    "test": "jest",
    "test:all": "set NODE_ENV=test && react-scripts test && npm run lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run build"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@testing-library/dom": "^7.26.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.2",
    "@testing-library/user-event": "^12.1.9",
    "@types/cookie-session": "^2.0.41",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.162",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.14.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-modal": "^3.10.6",
    "@types/rebass": "^4.0.7",
    "@types/rebass__forms": "^4.0.4",
    "@types/styled-components": "^5.1.4",
    "@types/styled-system": "^5.1.10",
    "@types/typography": "^0.16.3",
    "@typescript-eslint/eslint-plugin": "4.5.0",
    "@typescript-eslint/parser": "4.5.0",
    "@welldone-software/why-did-you-render": "^5.0.0-rc.1",
    "babel-eslint": "10.1.0",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-stylelint": "^13.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.0",
    "jest-styled-components": "^7.0.3",
    "prettier": "prettier/prettier",
    "pretty-quick": "^3.1.0",
    "react-test-renderer": "^17.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
