    "analytics": "^0.6.6",
    "array-move": "^3.0.1",
    "axios": "^0.21.1",
    "babel-jest": "^26.6.3",
    "body-scroll-lock": "^3.1.5",
    "clipboard": "^2.0.6",
    "colors": "^1.4.0",
    "envalid": "^6.0.2",
    "formik": "^2.2.6",
    "framer-motion": "^3.2.1",
    "gatsby-plugin-google-analytics": "^2.10.0",
    "jest": "^26.6.3",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.11.12",
    "next": "^10.0.5",
    "next-connect": "^0.9.1",
    "next-cookies": "^2.0.3",
    "polished": "^4.0.5",
    "prism-react-renderer": "^1.1.1",
    "react": "^17.0.1",
    "react-beautiful-dnd": "^13.0.0",
