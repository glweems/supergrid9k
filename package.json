{
  "name": "supergrid9k",
  "version": "0.0.1",
  "homepage": "https://supergrid9k.dev",
  "description": "A visual way to generate css for all your css-grid layouts.",
  "keywords": [
    "css",
    "grid",
    "css-grid",
    "generator",
    "layout",
    "create"
  ],
  "dependencies": {
    "@analytics/google-analytics": "^0.5.2",
