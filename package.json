{
  "name": "supergrid9k",
  "description": "css grid generator that is super and 9k",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "formik": "^2.1.5",
    "framer-motion": "^2.3.0",
    "lodash": "^4.17.19",
    "normalize-css": "^2.3.1",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-error-boundary": "^2.3.1",
    "react-is": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "recoil": "^0.0.10",
    "shortid": "^2.2.15",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format": "prettier --ignore-path .gitignore --write \"src/**/*.{ts,tsx,json,yml,md}\"",
    "lint:js": "eslint --ignore-path .gitignore \"src/**/*.{ts,tsx}\"",
    "lint:style": "stylelint --ignore-path .gitignore \"src/**/*.{ts,tsx}\"",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:fix": "npm run format && npm run lint:js --fix && npm run lint:style",
    "test": "react-scripts test",
    "test:all": "set NODE_ENV=test && react-scripts test && npm run lint",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test:all && npm run build"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "^7.21.7",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.1.0",
    "jest-styled-components": "^7.0.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@babel/register": "^7.10.5",
    "@types/lodash": "^4.14.158",
    "@types/react-router-dom": "^5.1.5",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.2",
    "@types/styled-system": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "3.8.0",
    "@typescript-eslint/parser": "3.8.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "eslint": "7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-config-stylelint": "^12.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.5",
    "eslint-plugin-react-hooks": "4.0.8",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react-dev-utils": "^10.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
